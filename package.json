{
  "name": "rathunt",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "bs-platform": "9.0.2",
    "bucklescript-tea": "^0.15.0",
    "file-system": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "reason-nodejs": "^13.0.0-beta.1",
    "rollup": "^2.58.0",
    "rollup-plugin-brfs": "^1.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "serve": "^12.0.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "serve": "serve release",
    "clean": "bsb -clean-world",
